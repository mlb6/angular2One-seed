{
  "traceur": {
    "dev" : {
      "modules": "amd",
      "script": false,
      "types": true,
      "typeAssertions": true,
      "typeAssertionModule": "assert",
      "annotations": true,
      "memberVariables":true,
      "debug": true,
      "sourcemaps":"file",
      "outputLanguage": "es5"
    },
    "devCommonjs" : {
      "modules": "commonjs",
      "script": false,
      "types": true,
      "annotations": true,
      "memberVariables":true,
      "outputLanguage": "es5"
    },
    "prodEs5" : {
      "modules": "amd",
      "script": false,
      "types": true,
      "typeAssertions": false,
      "annotations": true,
      "memberVariables": true,
      "outputLanguage": "es5"
    },
    "prodEs6" : {
      "modules": "amd",
      "script": false,
      "types": true,
      "typeAssertions": false,
      "annotations": true,
      "memberVariables": true,
      "outputLanguage": "es6"
    }
  },
  "modules" : {
    "configFile" : "src/main/require.config.js",
    "include" : {
      "dev": ["angular"],
      "prod": ["angular"]
    }
  },

  "path" : {
    "src" : {
      "baseOpt" : { "base" : "src"},
      "main" : "src/main",
      "unit" : "src/test/unit",

      "sass" : "src/main/**/*.scss",
      "atScript" : {
        "amd" : ["src/main/**/*.ats", "src/test/unit/**/*.ats"],
        "commonjs" : "src/test/e2e/**/*.ats"
      },
      "js" : "src/main/**/*.js",
      "appScripts" : ["src/main/**/*.js","src/main/**/*.ats"]
    },
    "dest" : {
      "build" : "build",
      "buildMain" : "build/main",
      "buildUnit" : "build/test/unit",
      "dist" : "dist",
      "maps" : "maps",
      "coverage" : "build/coverage"
    },
    "karmaConfig" : "src/test/karma.conf.js",
    "protractorConfig" : "src/test/protractor.conf.js",
    "jshint" : ["src/main/**/*.js", "src/test/unit/**/*.js","scripts/gulp/*.js", "build/main/**/*.js"]
  },

